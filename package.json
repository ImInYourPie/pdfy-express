		"@types/node": "^18.16.1",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"nodemon": "^2.0.22",
		"nodemon-webpack-plugin": "^4.8.1",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"webpack": "^5.81.0",
