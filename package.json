		"docker:stop": "docker-compose down",
		"docker:rebuild-dev": "docker-compose -f docker-compose.dev.yml rebuild",
		"build:dev": "NODE_ENV=development webpack --watch",
		"build:prod": "NODE_ENV=production webpack"
	},
